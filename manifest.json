{
  "name": "YouTube Ad Audio Blocker",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Mutes audio during YouTube ads",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["*://*.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
